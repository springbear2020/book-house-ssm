<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.whut.bear.panda.dao.AdminMapper">
    <!-- Admin getAdminByUsernameAndPassword(@Param("username") String username, @Param("password") String password); -->
    <select id="getAdminByUsernameAndPassword" resultType="Admin">
        select *
        from t_admin
        where username = #{username}
          and password = #{password}
    </select>
</mapper>